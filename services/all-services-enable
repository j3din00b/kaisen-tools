sudo systemctl start bacula-fd
sudo systemctl enable bacula-sd
sudo systemctl enable bettercap
sudo systemctl start bettercap
sudo systemctl enable bluetooth
sudo systemctl start bluetooth
sudo systemctl enable blueman-mechanism
sudo systemctl start blueman-mechanism
sudo systemctl enable ceph-fuse.target
sudo systemctl start ceph-fuse.target
