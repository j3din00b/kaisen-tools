sudo systemctl start lm-sensors
sudo systemctl enable lockdown
sudo systemctl start lockdown
sudo systemctl enable lxcfs
sudo systemctl start lxcfs
sudo systemctl enable lxc-net
sudo systemctl start lxc-net
sudo systemctl enable lxc
sudo systemctl start lxc
sudo systemctl enable neo4j
sudo systemctl start neo4j
sudo systemctl enable nfs-client.target
sudo systemctl start nfs-client.target
sudo systemctl enable nfs-kernel-server
sudo systemctl start nfs-kernel-server
sudo systemctl enable nfs-server
sudo systemctl start nfs-server
sudo systemctl enable portmap
sudo systemctl start portmap
sudo systemctl enable rpcbind
sudo systemctl start rpcbind
sudo systemctl enable rpcbind.socket
sudo systemctl start rpcbind.socket
sudo systemctl enable ntp
sudo systemctl start ntp
sudo systemctl enable o2cb
sudo systemctl start o2cb
sudo systemctl enable ocfs2
sudo systemctl start ocfs2
sudo systemctl enable openvpn
sudo systemctl start openvpn
sudo systemctl enable rpcbind
sudo systemctl start rpcbind
sudo systemctl enable rpcbind.socket
sudo systemctl start rpcbind.socket
sudo systemctl enable rsync
sudo systemctl start rsync
sudo systemctl enable setserial
sudo systemctl start setserial
sudo systemctl enable smartmontools
sudo systemctl start smartmontools
sudo systemctl enable ssh
sudo systemctl start ssh
sudo systemctl enable sysstat
sudo systemctl start sysstat
sudo systemctl enable teamviewerd
sudo systemctl start teamviewerd
sudo systemctl enable vboxweb
sudo systemctl enable virtualbox
